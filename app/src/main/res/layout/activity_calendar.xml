<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app = "http://schemas.android.com/apk/res-auto"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <!--自定义xmlns配置  xmlns:NewCalendarNS = "http://schemas.android.com/apk/res/com.example.jinhui.androiddemo"-->
    <com.example.jinhui.androiddemo.day11_customview.calendar.NewCalendar
        xmlns:NewCalendarNS = "http://schemas.android.com/apk/res/com.example.jinhui.androiddemo"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/newCalendar"
        NewCalendarNS:dateFormat = "MMMM yyyy"/>
</LinearLayout>